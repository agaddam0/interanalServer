global class FormSelection implements Comparable {
    
    public String Name						{get;set;}
    public String FormId					{get;set;}
    public String RecordTypeName			{get;set;}
    public String RecordTypeId				{get;set;}
    public boolean selected					{get;set;}
    public String PDFlink					{get;set;}
    public boolean AutoGenerated			{get;set;}
    public String Status					{get;set;}
    public String Tagline					{get;set;}
    public Integer sortNum					{get;set;}
    
    public FormSelection(String n, String rtn, String rtId)
    {
        Name = n;
        RecordTypeName = rtn;
        RecordTypeId = rtId;
        selected = false;
    }
    
    public FormSelection(String i, String n, String rtn, String s, String link, boolean ag, String tag, Integer st)
    {
        Name = n;
        FormId = i;
        RecordTypeName = rtn;
        PDFlink = link;
        AutoGenerated = ag;
        Status = s;
        Tagline = tag;
        sortNum = st;
    }

    global Integer compareTo(object objToCompare)
    {
        formSelection compareTo = (formSelection) objToCompare;
        integer returnValue = 0;
        
        if(sortNum > compareTo.sortNum)
            returnValue = 1;
        else if(sortNum < compareTo.sortNum)
            returnValue = -1;
        
        return returnValue;
    }
}