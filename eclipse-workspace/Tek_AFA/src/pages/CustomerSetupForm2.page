                function getAgentId()
			    {
			        var UserId = $('input[id$="MainAgentName_lkid"]').val();
			        var usr = new RemoteObjectModel.User();
			        usr.retrieve(
			            {where:
			             {Id: {eq: UserId}}},
			            function(err, usrs){
			                if(err){
			                	$('span[id$="MainAgentId"]').html('');
			                }else{
			                    usrs.forEach(function(u){
			                    	var agentId = u.get("Agent_ID__c");
			                    	if(agentId != null)
			                        	$('span[id$="MainAgentId"]').html(agentId);
			                        else
			                        	$('span[id$="MainAgentId"]').html('');
			                    });
			                }
			            });
			    }   
                
