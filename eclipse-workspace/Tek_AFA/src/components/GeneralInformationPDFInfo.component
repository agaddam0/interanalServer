    <table class="formtable" style="100%">
    
    <td style="width:50%">
    
        <table id="payrollInformation" class="formtable" >
            <tr>
                <td class="fieldLabel" ><div class="requiredLabel">{! $ObjectType.Platform_Setup__c.Fields.Enroll_Info_First_Payroll_Deduction_Date__c.Label }</div></td>
            </tr>
            <tr>
                <td><apex:outputField value="{!ps.Enroll_Info_First_Payroll_Deduction_Date__c}"  style="width:220px" /></td>
            </tr>
