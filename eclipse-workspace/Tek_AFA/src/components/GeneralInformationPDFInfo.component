            <apex:repeat value="{!oppTeam}" var="o">
                <tr>
                    <td style="width:25%;border-bottom:.5pt solid gray;">
                        <apex:outputField value="{!o.User.Name}" style="width:100%"/>
                    </td>
                    <td style="border-bottom:.5px solid gray;">
                        <apex:outputField value="{!o.User.Network_ID__c}" style="width:90%"/>
                    </td>
                    <td style="border-bottom:.5px solid gray;">
                        <apex:outputField value="{!o.User.Agent_ID__c}" style="width:90%"/>
                    </td>
                </tr>
            </apex:repeat>
        </tbody>
    </table>
    <br/>
    
    
    <apex:repeat value="{!EnrollmentContacts}" var="enrollmentContact">
        <div class="sectionHeader" style="margin-top:20px">
            <apex:outputText styleclass="sectionLabel" value="{! enrollmentContact.ContactType }"/>
        </div>
        
        <table class="formtable"> 
            <apex:outputPanel layout="none" rendered="{! enrollmentContact.HasSameAsAdminContactField == true }">
            <tr>
                <td class="fieldLabel">Same as Admin Contact?</td>
            </tr>
            <tr>
                <td><apex:outputText value="{! enrollmentContact.SameAsAdminContactYesNo }" /></td>                
            </tr>
            </apex:outputPanel>

            <tr>
                <td class="fieldLabel">Name</td>
            </tr>
            <tr>
                <td><apex:outputField value="{! enrollmentContact.Record.Name }" /></td>
            </tr>
            <tr>
                <td class="fieldLabel">Title</td>
            </tr>
            <tr>
                <td><apex:outputField value="{! enrollmentContact.Record.Title }"/></td>
            </tr>
            <tr>
                <td class="fieldLabel">Email</td>
            </tr>
            <tr>
                <td><apex:outputField value="{! enrollmentContact.Record.Email }"/></td>
            </tr>
            <tr>
                <td class="fieldLabel">Phone</td>
            </tr>
            <tr>
                <td><apex:outputField value="{! enrollmentContact.Record.Phone }" /></td>
            </tr>
        </table>
    </apex:repeat>
    
    <div class="pageBreak" />
</apex:component>