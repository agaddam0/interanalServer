                <td class="fieldLabel"><div class="requiredLabel">{!$ObjectType.Platform_Setup__c.Fields.Enroll_Info_Payroll_Company__c.Label}</div></td>
            </tr>
            <tr>
                <td><apex:outputField value="{!ps.Enroll_Info_Payroll_Company__c}" style="width:220px"/></td>
            </tr>
            
