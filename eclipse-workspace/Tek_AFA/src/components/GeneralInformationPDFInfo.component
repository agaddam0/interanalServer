                <td class="fieldLabel" ><div class="requiredLabel">{! $ObjectType.Platform_Setup__c.Fields.Enroll_Info_Payroll_Deduction_Begins__c.Label }</div></td>
            </tr>
            <tr>
                <td><apex:outputField value="{!ps.Enroll_Info_Payroll_Deduction_Begins__c}"   style="width:220px" /></td>
            </tr>
            </apex:outputPanel>
            
            <apex:outputPanel layout="none" rendered="{! showPayrollDeductionFrequency }">
