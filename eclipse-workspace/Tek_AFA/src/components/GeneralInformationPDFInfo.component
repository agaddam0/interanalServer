             <div>
	             <div><apex:outputField value="{!ps.Enroll_Info_Upload__c}" /></div>
	         </div>

             <apex:outputPanel layout="none" rendered="{! ps.Enroll_Info_Upload__c == 'Yes' }">
             <div id="payrollFileUploadDueDateLabelRow">
	             <div class="fieldLabel"><apex:outputText value="{! $ObjectType.Platform_Setup__c.Fields.EnrollInfoUploadDueDate__c.Label }" /></div>
	         </div>
	         <div id="payrollFileUploadDueDateInputRow">
	             <apex:outputField value="{!ps.EnrollInfoUploadDueDate__c}" />
	         </div>
	         </apex:outputPanel>
         </td>
     </table>
   
