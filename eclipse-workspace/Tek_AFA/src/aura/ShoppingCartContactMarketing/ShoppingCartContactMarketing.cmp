<aura:component>
    <div class="slds-box slds-theme_default oneContent active">
        <div class="slds-grid slds-gutters af-header slds-p-bottom_medium slds-p-horizontal_large">
            <div><img src="{!$Resource.ShoppingCartIcons + '/Icons/Contact_Marketing.png'}" class="af-contact-marketing-icon slds-horizontal-left_medium" /></div>
            <div class="slds-align_absolute-center af-header-title">
                <p class="af-contact-marketing-title slds-text-heading_medium">Contact <br /> Marketing</p>
            </div>
        </div>

        <!-- Contact Marketing Body -->
        <div class="slds-p-around_medium">
            <div>
                <p class="slds-m-bottom_medium">{! $Label.c.Marketplace_Contact_Marketing_Description }</p>

                <lightning:button label="Contact Us"
                                  variant="neutral"
                                  class="af-contact-us-button"
                                  onclick="{! c.startContactUsFlow }" />
            </div>
        </div>
    </div>

</aura:component>