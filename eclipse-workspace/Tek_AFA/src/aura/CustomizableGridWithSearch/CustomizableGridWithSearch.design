<design:component >
    <design:attribute name="objectName" label="SObject" description="The API name of the SObject." required="true" />
    <design:attribute name="fieldSetColumns" label="Field Set" description="The API name of the field set to use on the given SObject." required="true" />
    <design:attribute name="useOwnerId" label="Filter Logged in User" description="Shows records owned by the user that is currently logged in." />
    <design:attribute name="ProTraxUser" label="Filter by the User of the Protrax record" description="FOR PROTRAX ONLY: Shows results based on the User tied to the Protrax record." />
